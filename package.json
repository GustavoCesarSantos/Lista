{
  "name": "api",
  "version": "1.0.0",
  "description": "Projeto para estudo de back-end",
  "repository": "https://github.com/GustavoCesarSantos/Lista",
  "main": "server.js",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "npm run test --bail --findRelatedTests",
      "git add"
    ]
  },
  "scripts": {
    "pretest": "set NODE_ENV=test&&sequelize db:migrate",
    "test": "set NODE_ENV=test&&jest --no-cache --runInBand",
    "test:cov": "set NODE_ENV=test&&jest --no-cache --runInBand --coverage",
    "posttest": "set NODE_ENV=test&&sequelize db:migrate:undo:all",
    "start": "sequelize db:migrate&&node server.js",
    "dev": "set NODE_ENV=development&&sequelize db:migrate&&nodemon server.js --ignore __tests__",
    "swagger-autogen": "node ./src/config/doc/swagger.js"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.0.0",
    "cors": "^2.8.5",
    "date-fns": "^2.16.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "helmet": "^4.3.1",
    "joi": "^17.3.0",
    "jsonwebtoken": "^8.5.1",
    "mysql2": "^2.2.5",
    "nodemailer": "^6.4.17",
    "passport": "^0.4.1",
    "passport-http-bearer": "^1.0.1",
    "passport-local": "^1.0.0",
    "path": "^0.12.7",
    "redis": "^3.0.2",
    "sequelize": "^6.3.5",
    "swagger-autogen": "^2.3.6",
    "swagger-ui-express": "^4.1.6",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "eslint": "^7.15.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "husky": "^7.0.4",
    "jest": "^27.3.1",
    "lint-staged": "^12.1.2",
    "nodemon": "^2.0.6",
    "sequelize-cli": "^6.2.0",
    "sequelize-mock": "^0.10.2",
    "sqlite3": "^5.0.2",
    "supertest": "^6.1.3"
  }
}
